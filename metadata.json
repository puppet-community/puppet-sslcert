{
  "name": "puppetlabs-sslcertificate",
  "version": "5.0.1",
  "author": "puppetlabs",
  "license": "MIT",
  "summary": "Module to manage SSL Certificates on Windows Server 2008 and upwards",
  "source": "https://github.com/puppetlabs/puppetlabs-sslcertificate",
  "project_page": "https://github.com/puppetlabs/puppetlabs-sslcertificate",
  "issues_url": "https://github.com/puppetlabs/puppetlabs-sslcertificate/issues",
  "operatingsystem_support": [
    {
